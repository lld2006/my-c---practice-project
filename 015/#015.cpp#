#include <cstdio>
#include <array>
#include "../lib/typedef.h"
using namespace std;
#define nmax 4
i64 search_pathes( int row, int col, array<array<i64,nmax>, nmax>& a)
{
    if(a[row][col] > 0 ) 
        return a[row][col];
    i64 value = search_pathes(row-1, col, a) + 
        search_pathes(row, col-1, a);
    a[row][col] = value;
    return value;
}
int main()
{
    array<array<i64,nmax>, nmax>        a;
    for( int i = 0; i < nmax; ++i)
        for( int j = 0; j < nmax; ++j){
            a[i][j] = 0;
        }
    for(int i = 0; i < nmax; ++i){
        a[i][0] = 1;
        a[0][i] = 1;
    }
    i64 value = search_pathes(nmax, nmax, a);
    printf("%lld\n", value);
}
