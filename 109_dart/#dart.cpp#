#include <cstdio>
#include <vector>
#include <cassert>
using namespace std;

vector<int> single;
vector<int> dbl;
vector<int> triple;

int check3(int total, int limit);
int check2(int total, int limit);
int check1(int total, int limit);

int freeThrow(int total, int limit){
    if(total < 0) return 0;
    if(total == 0) return 1;
    assert(limit >= 0 && limit <= 3);
    int sum  = 0;
    if(limit ==3 && total >=3) //can use all level
        sum += check3(total, 3);
    if(limit >= 2 && total >= 2)//up to two level
        sum += check2(total, 2);
    if(limit >= 1) // only level one
        sum += check1(total, 1);
    return limit;
}
int check3(int total, int limit ){
    assert(limit == 3);
    int sum = 0;
    for(unsigned int i = 0; i < triple.size(); ++i){
        sum += freeThrow(total - triple[i], limit) ;
    }
    return sum;
}
int check2(int total, int limit ){
    assert(limit == 2);
    int sum = 0;
    for(unsigned int i = 0; i < dbl.size(); ++i){
        sum += freeThrow(total - dbl[i], limit);
    }
    return sum;
}
int check1(int total, int limit ){
    assert(limit == 1);
    int sum = 0;
    for(unsigned int i = 0; i < single.size(); ++i){
        sum += freeThrow(total - single[i], limit) ;
    }
    return sum;
}
int main(){
    single.resize(21);
    dbl.resize(21);
    triple.resize(20);
    for(unsigned int i = 0; i < 20; ++i){
        single[i] = i+1;
        dbl[i] = (i+1)*2;
        triple[i] = (i+1)*3;
    }
    dbl[20] = 50;
    single[20] = 25;
    int count = 0;
    for(unsigned int i = 0; i< dbl.size(); ++i){
        //                  total       level constrain score constrain
        count += freeThrow( 6 - dbl[i], 3 );
    }
    printf("%d\n", count);
}
